@model List<IEOtherDetailViewModel>

<form id="frmList" action="Save" method="post">
     <input type="hidden"   name="TotalQty" id="ViewBagTotalQty"/>
     <input type="hidden"   name="TotalPrice" id="ViewBagTotalPrice"/>
    <input type="hidden"   name="TotalShippingWeight" id="ViewBagTotalShippingWeight"/>
    <div class="form-horizontal">
        <table id="tblImportDetail" class="table table-bordered table-striped table-hover">
            <thead>
                <tr class="warning">
                    <th style="width:40px" class="text-center">STT</th>
                    <th style="width:100px">Sản phẩm</th>
                    <th style="width:110px">Giá</th>
                    <th style="width:100px">Số lượng</th>
                    <th style="width:130px">Thành tiền</th>
                    <th style="width:130px">Trọng lượng</th>
                    <th style="width:130px">Ghi chú</th>
                    <th style="width:100px">@Resources.LanguageResource.Actions</th>
                </tr>
            </thead>
            <tbody>
               @Html.Partial("_CreateListInner", Model)
            </tbody>
            <tfoot>
                <tr>
                    <td class="text-right" colspan="3"><strong>Tổng cộng:</strong></td>
                    @*<td class="text-right" id="SumPrice" ></td>*@
                    <td class="text-right" id="SumQty"></td>
                     <td></td>
                    <td></td>
                     <td></td>
                    <td></td>
                </tr>
                @*<tr>
                    <td class="text-right" colspan="4"><strong>Số tiền khách trả:</strong></td>
                     <td class="text-right"  >
                        <input type="text" name="Money" id="Money" class="form-control text-right"  />
                    </td><td></td>
                    <td></td>
                     <td></td>
                </tr>*@
                <tr>
                    <td colspan="8">
                        <div class="col-md-4">
                            <div class="col-md-4">
                                <input type="text" id="ProQty" name="ProQty" class="form-control text-center " value="1" />
                            </div>
                            <div class="col-md-8">
                                <input type="text" id="scanBarcode" name="scanBarcode" class="form-control " placeholder="Quét mã vạch.." />
                            </div>
                        </div>
                        <button id="btnAddNewRow" type="button" class="btn btn-default" style="float:right">Thêm mặt hàng</button>
                    </td>
                </tr>
            </tfoot>
        </table>
    </div>
</form>

