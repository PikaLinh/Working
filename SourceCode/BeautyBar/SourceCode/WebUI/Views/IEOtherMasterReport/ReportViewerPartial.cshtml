@using WebUI.Report

@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer";
    settings.Report = (HoaDonXuatNhapKhacXtraReport)ViewData["Report"];
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Action = "CallbackReportViewerPartial", Controller = "IEOtherMasterReport", IEOtherMasterId = ViewBag.IEOtherMasterId };
    settings.ExportRouteValues = new { Action = "ExportReportViewerPartial", Controller = "IEOtherMasterReport", IEOtherMasterId = ViewBag.IEOtherMasterId };
}).GetHtml()

