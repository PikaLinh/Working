@using WebUI.Report

@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer";
    settings.Report = (HoaDonNhapNhaCungCapXtraReport)ViewData["Report"];
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Action = "CallbackReportViewerPartial", Controller = "ImportMasterReport", ImportMasterId = ViewBag.ImportMasterId };
    settings.ExportRouteValues = new { Action = "ExportReportViewerPartial", Controller = "ImportMasterReport", ImportMasterId = ViewBag.ImportMasterId };
}).GetHtml()

