@using WebUI.Report

@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer";
    settings.Report = (HoaDonKhachHangTraHangXtraReport1)ViewData["Report"];
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Action = "CallbackReportViewerPartial", Controller = "OrderReturnReportGetData", OrderReturnMasterId = ViewBag.OrderReturnMasterId };
    settings.ExportRouteValues = new { Action = "ExportReportViewerPartial", Controller = "OrderReturnReportGetData", OrderReturnMasterId = ViewBag.OrderReturnMasterId };
}).GetHtml()

