@using WebUI.Report

@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer";
    settings.Report = (HoaDonYeuCauNhapHangTuNhaCungCapXtraReport)ViewData["Report"];
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Action = "CallbackReportViewerPartial", Controller = "PreImportReportGetData", PreImportMasterId = ViewBag.PreImportMasterId };
    settings.ExportRouteValues = new { Action = "ExportReportViewerPartial", Controller = "PreImportReportGetData", PreImportMasterId = ViewBag.PreImportMasterId };
}).GetHtml()

