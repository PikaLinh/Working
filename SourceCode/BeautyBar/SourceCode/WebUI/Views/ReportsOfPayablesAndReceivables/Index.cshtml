@{
    ViewBag.Title = "Báo cáo công nợ";
}
<div id="tabs">
    <ul id="nav-tab-list" class="nav nav-tabs">
        <li id="tab-1">
            <a href="#pane-1" data-toggle="tab">Công nợ khách hàng</a>
        </li>
        <li id="tab-2">
            <a href="#pane-2" data-toggle="tab">Công nợ nhà cung cấp</a>
        </li>
    </ul>
    <input type="hidden" id="num" name="num" />
    <!-- Tab Pane - Contents -->
    <div id="tab-content" class="tab-content">
        <div id="pane-1" class="tab-pane">
            <div class=" control-list">
                <div id="divCustomer" class="control-group">
                    @Html.Partial("_Customer")
                </div>
            </div>
        </div>
        <div id="pane-2" class="tab-pane">
              <div class=" control-list">
                <div id="divSuplier" class="control-group">
                      @Html.Partial("_Suplier")
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
<script src="~/Scripts/ReportsOfPayablesAndReceivables/Index.js"></script>
}
