<div class="panel text-center">
    <div class="panel-body">
        @Html.DevExpress().ReportToolbar(settings =>
        {
            settings.Name = "ReportViewerToolbar";
            settings.ReportViewerName = "reportViewer";
        }).GetHtml()
        <div id="divReport" style="width: 1000px; margin: auto auto;">
            @Html.Action("ReportViewerPartial", "InventoryProductReportView", new { StoreId = ViewBag.StoreId, ToDate = ViewBag.ToDate, FromDate = ViewBag.FromDate ,SupplierId = ViewBag.SupplierId, EmployeeId = ViewBag.EmployeeId,WarehouseId= ViewBag.WarehouseId})
        </div>
    </div>
</div>
