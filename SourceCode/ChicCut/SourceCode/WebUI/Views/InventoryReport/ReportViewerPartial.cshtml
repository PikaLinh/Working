@using WebUI.Report

@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer";
    settings.Report = (DanhSachXuatNhapTonSPXtraReport)ViewData["Report"];
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Action = "CallbackReportViewerPartial", Controller = "InventoryReport" };
    settings.ExportRouteValues = new { Action = "ExportReportViewerPartial", Controller = "InventoryReport"};
}).GetHtml()
