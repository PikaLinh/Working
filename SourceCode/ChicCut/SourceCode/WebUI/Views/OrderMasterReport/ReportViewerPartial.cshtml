@using WebUI.Report

@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer";
    settings.Report = (HoaDonBanHangXtraReport1)ViewData["Report"];
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Action = "CallbackReportViewerPartial", Controller = "OrderMasterReport", OrderId = ViewBag.OrderId };
    settings.ExportRouteValues = new { Action = "ExportReportViewerPartial", Controller = "OrderMasterReport", OrderId = ViewBag.OrderId };
}).GetHtml()

