@using WebUI.Report

@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer";
    settings.Report = (HoaDonXtraReport)ViewData["Report"];
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Action = "CallbackReportViewerPartial", Controller = "OrderReport", OrderId = ViewBag.OrderId};
    settings.ExportRouteValues = new { Action = "ExportReportViewerPartial", Controller = "OrderReport", OrderId = ViewBag.OrderId};
}).GetHtml()

