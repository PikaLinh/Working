@using WebUI.Report

@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer";
    settings.Report = (HoaDonTraHangNhaCungCapXtraReport)ViewData["Report"];
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Action = "CallbackReportViewerPartial", Controller = "ReturnReportGetData", ReturnMasterId = ViewBag.ReturnMasterId };
    settings.ExportRouteValues = new { Action = "ExportReportViewerPartial", Controller = "ReturnReportGetData", ReturnMasterId = ViewBag.ReturnMasterId };
}).GetHtml()

