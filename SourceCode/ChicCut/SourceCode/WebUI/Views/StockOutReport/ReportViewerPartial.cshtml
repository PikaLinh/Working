@using WebUI.Report

@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer";
    settings.Report = (PhieuXuatKhoXtraReport)ViewData["Report"];
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Action = "CallbackReportViewerPartial", Controller = "StockOutReport", StoreId = ViewBag.StoreId, ToDate = ViewBag.ToDate, FromDate = ViewBag.FromDate, CustomerId = ViewBag.CustomerId, EmployeeId = ViewBag.EmployeeId };
    settings.ExportRouteValues = new { Action = "ExportReportViewerPartial", Controller = "StockOutReport", StoreId = ViewBag.StoreId, ToDate = ViewBag.ToDate, FromDate = ViewBag.FromDate, CustomerId = ViewBag.CustomerId, EmployeeId = ViewBag.EmployeeId };
}).GetHtml()

