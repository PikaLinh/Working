@{
    ViewBag.Title = "Kiểm kho";
}
<div id="tabs">
    <ul id="nav-tab-list" class="nav nav-tabs">
        <li id="tab-1">
            <a href="#pane-1" data-toggle="tab" id="SPHienCo">Sản phẩm hiện có</a>
        </li>
        <li id="tab-2">
            <a href="#pane-2" data-toggle="tab" id="PhieuKiemKho">Phiếu kiểm kho</a>
        </li>
        <li id="tab-3">
            <a href="#pane-3" data-toggle="tab" id="SPKiemKho">Sản phẩm kiểm kho</a>
        </li>
    </ul>
    <input type="hidden" id="num" name="num" />
    <!-- Tab Pane - Contents -->
    <div id="tab-content" class="tab-content">
        <div id="pane-1" class="tab-pane">
            <div class=" control-list">
                <div id="divProfile" class="control-group">
                   @Html.Partial("_Procduct")
                </div>
            </div>
        </div>
        <div id="pane-2" class="tab-pane">
            <div class="control-group">
                   @Html.Partial("_WarehouseInventory")
            </div>
        </div>
        <div id="pane-3" class="tab-pane">
            <div class="control-group">
                 @Html.Partial("_WarehouseInventoryProducts")
            </div>
        </div>
    </div>
</div>

@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/plugins/form-master/jquery.form.js"></script>
    <script src="~/Scripts/WarehouseInventory/Index.js"></script>
}
<div class="loading-modal"></div>